<h1>Medical History for <%= @person.full_name %></h1>

<ul>
    <% @medical_history.attributes.each do |k, v| %>
      <% if k != "id" && "person" %>
      <% if v != false %>
      <% if v.blank? != true %>
        <li><%= k %> : <%= v %></li>
    <% end %>
    <% end %>
    <% end %>
    <% end %>
    </br>
  </ul>




